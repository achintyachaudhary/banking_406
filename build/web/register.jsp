<body class="animsition">
    <div class="page-wrapper">
        <div class="page-content--bge5">
            <div class="container">
                <div class="login-wrap">
                    <div class="login-content">
                        <div class="login-form">
                            <form action="reg" method="post">
                                <div class="form-group">
                                    <label>Username</label>
                                    <input class="au-input au-input--full" type="text" name="username" placeholder="Username">
                                </div>
                                <div class="form-group">
                                    <label>Email Address</label>
                                    <input class="au-input au-input--full" type="email" name="email" placeholder="Email">
                                </div>
                                <div class="form-group">
                                    <label>Password</label>
                                    <input class="au-input au-input--full" type="password" name="password" placeholder="Password">
                                </div>
                                <div class="form-group">
                                    <label>Name</label>
                                    <input class="au-input au-input--full" type="text" name="name1" placeholder="Name">
                                </div>
                                <div class="form-group">
                                    <label>Address</label>
                                   <div class="col-12 col-md-9">
                                                    <textarea name="address" id="textarea-input" rows="2" placeholder="Address..." class="form-control"></textarea>
                                                </div>
                                </div>
                                <div class="form-group">
                                    <label>Gender</label>
                                    <div class="col-12 col-md-9">
                                                    <select name="gender" id="select" class="form-control">
                                                        <option value="0">Please select</option>
                                                        <option value="Male">Male</option>
                                                        <option value="Female">Female</option>
                                                        <option value="Others">Others</option>
                                                    </select>
                                                </div>
                                    
                                </div>
                               
                                
                                <button class="au-btn au-btn--block au-btn--green m-b-20" type="submit">Next Page</button>
                                
